                     , bool-extras
                     , containers
                     , data-ordlist
                     , hashable
                     , hashtables
                     , HUnit
                     , mtl
                     , parsec
                     , recursion-schemes
                     , test-hunit-util
                     , wl-pprint
  default-language:    Haskell2010
  ghc-options:         -Wall
